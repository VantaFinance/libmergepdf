{
    "name": "vanta/libmergepdf",
    "description": "Library for merging multiple PDFs",
    "keywords": ["pdf", "merge"],
    "type": "library",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Hannes Forsg√•rd",
            "email": "hannes.forsgard@fripost.org"
        },
        {
            "name": "Vanta Team",
            "homepage": "https://vanta.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "iio\\libmergepdf\\": "src/"
        },
        "classmap": [
            "tcpdi/"
        ]
    },
    "require": {
        "php": "^7.4||^8.0",
        "tecnickcom/tcpdf": "^6.9",
        "setasign/fpdi": "^2.6"
    },
    "conflict": {
        "setasign/fpdf": "*",
        "rafikhaceb/tcpdi": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "smalot/pdfparser": "~0.13",
        "phpspec/prophecy": "^1.22",
        "behat/behat": "^3.15",
        "friendsofphp/php-cs-fixer":  "3.75",
        "phpstan/phpstan": "^2.1",
        "jangregor/phpstan-prophecy": "^2.1",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpspec/prophecy-phpunit": "^2.3"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
